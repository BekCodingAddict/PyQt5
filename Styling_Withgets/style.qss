QLabel {
    color:white;
    font-size:24px;
    font-family:"Arial";
    text-align:center;
    background-color: #FFFFFF;
    qproperty-alignment: AlignCenter;
}

QListWidget {
    color: #FFFFFF;
    background-color: #33373B;
}

QListWidget::item {
    height: 50px;
}

QListWidget::item:selected {
    background-color: #2ABf9E;
}

QPushButton {
    background-color: #2ABf9E;
    padding: 20px;
    font-size: 18px;
}